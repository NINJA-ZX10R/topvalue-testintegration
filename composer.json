{
    "name": "magento/project-community-edition",
    "description": "Magento Open Source",
    "require": {
      "magento/framework": "^2.4",
      "php": ">=7.4.3",
      "composer/composer": "^2.1",
      "topvalue/testintegration": "*"
    },
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/NINJA-ZX10R/topvalue-testintegration"
      }
    ],
    "config": {
      "preferred-install": "dist",
      "use-include-path": true
    },
    "autoload": {
      "psr-4": {
        "Magento\\": "vendor/magento/framework/src/Magento/"
      }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
  }
  
  